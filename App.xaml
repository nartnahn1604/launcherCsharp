<Application x:Class="Launcher.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Launcher"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <FontFamily x:Key="UTM AS SignWriter">pack://application:,,,/Fonts/#UTM A&amp;S Signwriter</FontFamily>
        <FontFamily x:Key="Myriad Arabic">pack://application:,,,/Fonts/#Myriad Arabic Regular</FontFamily>
    </Application.Resources>
</Application>
